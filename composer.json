{
    "name": "dougl/jhonesframework",
    "description": "Estrutura base para meus projetos",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "douglasjoanes",
            "email": "douglasjoanes2@hotmail.com"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "altorouter/altorouter": "^2.0",
        "phpmailer/phpmailer": "^6.5",
        "twig/twig": "^3.3",
        "twbs/bootstrap": "^5.1",
        "matthiasmullie/minify": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/support/Minify.php",
            "src/helpers/hDefines.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
